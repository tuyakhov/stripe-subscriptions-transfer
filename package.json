{
  "name": "stripe-subscriptions-transfer",
  "description": "Transfer subscriptions from one account to another",
  "version": "0.0.0",
  "author": "Anton Tuyakhov @tuyakhov",
  "bin": {
    "stripe-transfer": "./bin/run"
  },
  "bugs": "https://github.com/tuyakhov/stripe-subscriptions-transfer/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.6.3",
    "stripe": "^8.165.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/tuyakhov/stripe-subscriptions-transfer",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "stripe-transfer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tuyakhov/stripe-subscriptions-transfer",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
